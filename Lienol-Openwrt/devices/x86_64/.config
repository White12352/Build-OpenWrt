#
# Target Images
#
CONFIG_TARGET_x86=y
CONFIG_TARGET_x86_64=y
CONFIG_TARGET_x86_64_Generic=y
CONFIG_TARGET_IMAGES_GZIP=y
CONFIG_TARGET_KERNEL_PARTSIZE=48
CONFIG_TARGET_ROOTFS_PARTSIZE=208
CONFIG_TARGET_ROOTFS_PARTNAME=""
# CONFIG_TARGET_ROOTFS_PERSIST_VAR is not set
# end of Target Images

# IMAGEOPT
CONFIG_IMAGEOPT=y
CONFIG_PREINITOPT=y
CONFIG_TARGET_PREINIT_IFNAME=""
CONFIG_TARGET_PREINIT_IP="10.11.12.2"
CONFIG_TARGET_PREINIT_NETMASK="255.255.255.0"
CONFIG_TARGET_PREINIT_BROADCAST="10.11.12.255"
CONFIG_INITOPT=y
CONFIG_TARGET_INIT_PATH="/usr/sbin:/usr/bin:/sbin:/bin"
CONFIG_TARGET_INIT_ENV=""
CONFIG_TARGET_INIT_CMD="/sbin/init"
CONFIG_TARGET_INIT_SUPPRESS_STDERR=y
CONFIG_VERSIONOPT=y
CONFIG_VERSION_DIST="Lienol-Openwrt"
CONFIG_VERSION_NUMBER="22.03"
CONFIG_VERSION_CODE="Jammy"
CONFIG_VERSION_REPO="http://downloads.openwrt.org/snapshots"
CONFIG_VERSION_HOME_URL="https://github.com/Lienol/openwrt"
CONFIG_VERSION_MANUFACTURER="Lienol-Openwrt"
CONFIG_VERSION_MANUFACTURER_URL="https://github.com/Lienol/openwrt"
CONFIG_VERSION_BUG_URL="https://github.com/Lienol/openwrt"
CONFIG_VERSION_SUPPORT_URL="https://github.com/Lienol/openwrt"
CONFIG_VERSION_PRODUCT="Lienol"
CONFIG_VERSION_HWREV=""
CONFIG_VERSION_FILENAMES=n
CONFIG_VERSION_CODE_FILENAMES=y
CONFIG_PER_FEED_REPO=y
CONFIG_FEED_packages=y
CONFIG_FEED_luci=y
CONFIG_FEED_routing=y
CONFIG_FEED_telephony=y
# CONFIG_FEED_lienol  is not set
# CONFIG_FEED_other  is not set
# CONFIG_FEED_zhuxiaole is not set
# CONFIG_FEED_onlyme is not set

# system
CONFIG_PACKAGE_default-settings=y
CONFIG_LUCI_LANG_zh_Hans=y
CONFIG_PACKAGE_curl=y
CONFIG_PACKAGE_htop=y
CONFIG_PACKAGE_nano=y
CONFIG_PACKAGE_tree=y
CONFIG_PACKAGE_wget=y
CONFIG_PACKAGE_snmpd=y
CONFIG_PACKAGE_uhttpd=y
CONFIG_PACKAGE_ndppd=y
CONFIG_PACKAGE_dnsmasq=n
CONFIG_PACKAGE_dnsmasq-full=y
CONFIG_PACKAGE_luci-mod-admin-full=y
CONFIG_PACKAGE_luci-base=y
CONFIG_PACKAGE_luci-i18n-base-zh-cn=y
CONFIG_PACKAGE_luci-app-opkg=y
CONFIG_PACKAGE_luci-i18n-opkg-zh-cn=y

# ipv6
CONFIG_PACKAGE_ipv6helper=y

# 固件在线升级(仅自用)
CONFIG_PACKAGE_luci-app-gpsysupgrade=y
CONFIG_PACKAGE_luci-i18n-gpsysupgrade-zh-cn=y

# 设置向导
CONFIG_PACKAGE_luci-app-wizard=y
CONFIG_PACKAGE_luci-i18n-wizard-zh-cn=y

# airplay2
CONFIG_PACKAGE_luci-app-airplay2=y
CONFIG_PACKAGE_luci-i18n-airplay2-zh-cn=y

# 支持计划重启
CONFIG_PACKAGE_luci-app-autoreboot=y
CONFIG_PACKAGE_luci-i18n-autoreboot-zh-cn=y

# 防火墙
CONFIG_PACKAGE_luci-app-firewall=y
CONFIG_PACKAGE_luci-i18n-firewall-zh-cn=y

# QOS流控 Nftables版
CONFIG_PACKAGE_luci-app-nft-qos=n
CONFIG_PACKAGE_luci-i18n-nft-qos-zh-cn=n

# 简单QOS eqos
CONFIG_PACKAGE_luci-app-eqos=n
CONFIG_PACKAGE_luci-i18n-eqos-zh-cn=n

# frpc
CONFIG_PACKAGE_luci-app-frpc=y
CONFIG_PACKAGE_luci-i18n-frpc-zh-cn=y

# 高级设置
CONFIG_PACKAGE_luci-app-advanced=y
CONFIG_PACKAGE_luci-i18n-advanced-zh-cn=y

# ttyd终端
CONFIG_PACKAGE_luci-app-ttyd=y
CONFIG_PACKAGE_luci-i18n-ttyd-zh-cn=y

# nlbwmon监控
CONFIG_PACKAGE_luci-app-nlbwmon=n
CONFIG_PACKAGE_luci-i18n-nlbwmon-zh-cn=n

# wrtbwmon监控（serverchan需要）
CONFIG_PACKAGE_luci-app-wrtbwmon=y
CONFIG_PACKAGE_luci-i18n-wrtbwmon-zh-cn=y

# serverchan
CONFIG_PACKAGE_luci-app-serverchan=n
CONFIG_PACKAGE_luci-i18n-serverchan-zh-cn=n

# passwall
CONFIG_PACKAGE_v2ray-geodata=y
CONFIG_PACKAGE_luci-i18n-passwall-zh-cn=y
CONFIG_PACKAGE_luci-app-passwall=y
CONFIG_PACKAGE_luci-app-passwall_INCLUDE_ChinaDNS_NG=y
CONFIG_PACKAGE_luci-app-passwall_INCLUDE_IPv6_Nat=y
