# filetransfer文件传输
CONFIG_PACKAGE_luci-app-filetransfer=y
CONFIG_PACKAGE_luci-i18n-filetransfer-zh-cn=y

# WiFi 计划
CONFIG_PACKAGE_luci-app-wifischedule=n
CONFIG_PACKAGE_luci-i18n-wifischedule-zh-cn=n

# WiFi访客网络
CONFIG_PACKAGE_luci-app-guest-wifi=n
CONFIG_PACKAGE_luci-i18n-guest-wifi-zh-cn=n

# smartdns
CONFIG_PACKAGE_luci-app-smartdns=n
CONFIG_PACKAGE_luci-i18n-smartdns-zh-cn=n

# adbyby
CONFIG_PACKAGE_luci-app-adbyby-plus=y
CONFIG_PACKAGE_luci-i18n-adbyby-plus-zh-cn=y

# adguardhome广告拦截
CONFIG_PACKAGE_luci-app-adguardhome=n
CONFIG_PACKAGE_luci-i18n-adguardhome-zh-cn=n

# 释放内存
