CONFIG_PACKAGE_luci-i18n-uugamebooster-zh-cn=n

# upnp
CONFIG_PACKAGE_luci-app-upnp=n
CONFIG_PACKAGE_luci-i18n-upnp-zh-cn=n

# wol网络唤醒
CONFIG_PACKAGE_luci-app-wolplus=y
CONFIG_PACKAGE_luci-i18n-wolplus-zh-cn=y

# appfilter应用过滤
CONFIG_PACKAGE_luci-app-oaf=n
CONFIG_PACKAGE_luci-i18n-oaf-zh-cn=n

# USB 打印服务器
CONFIG_PACKAGE_luci-app-usb-printer=n
CONFIG_PACKAGE_luci-i18n-usb-printer-zh-cn=n

# accesscontrol访问时间控制
CONFIG_PACKAGE_luci-app-accesscontrol=n
CONFIG_PACKAGE_luci-i18n-accesscontrol-zh-cn=n

# CIFS/SMB挂载设置
CONFIG_PACKAGE_luci-app-cifs-mount=y
CONFIG_PACKAGE_luci-i18n-cifs-mount-zh-cn=y

# CloudShark捕获工具
CONFIG_PACKAGE_luci-app-cshark=n
CONFIG_PACKAGE_luci-i18n-cshark-zh-cn=n

# 分布式AP管理程序
CONFIG_PACKAGE_luci-app-dawn=n
CONFIG_PACKAGE_luci-i18n-dawn-zh-cn=n

# easymesh 简单MESH(可有线+无线回程)
